{
  "name": "you-are-next",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "typecheck": "npx tsc --noEmit",
    "verify:phase1:static": "npm run typecheck && npm run build",
    "verify:phase1:runtime-smoke": "node scripts/verify-phase1-runtime-smoke.mjs",
    "verify:phase2:static": "npm run typecheck && npm run build",
    "verify:phase2:runtime-smoke": "node scripts/verify-phase2-runtime-smoke.mjs",
    "verify:phase25:static": "npm run typecheck && npm run build",
    "verify:phase25:runtime-smoke": "node scripts/verify-phase25-runtime-smoke.mjs",
    "verify:phase3:static": "npm run typecheck && npm run build",
    "verify:phase3:runtime-smoke": "node scripts/verify-phase3-runtime-smoke.mjs",
    "verify:phase4:static": "npm run typecheck && npm run build",
    "verify:phase4:authority": "node scripts/verify-phase4-authority.mjs",
    "verify:phase4:runtime-smoke": "node scripts/verify-phase4-runtime-smoke.mjs",
    "verify:phase5:static": "npm run typecheck && npm run build",
    "verify:phase5:bots": "node scripts/verify-phase5-bots.mjs",
    "verify:phase5:azure": "node --import tsx/esm scripts/verify-phase5-azure.mjs",
    "build": "vite build",
    "preview": "vite preview",
    "party:dev": "npx partykit dev",
    "party:deploy": "npx partykit deploy"
