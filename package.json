    "react-router-dom": "^7.0"
  },
  "devDependencies": {
    "@babylonjs/inspector": "^7.54.3",
    "@playwright/test": "^1.58.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.0",
    "partykit": "^0.0.108",
    "spectorjs": "^0.9.30",
    "tsx": "^4.0",
    "typescript": "^5.5",
    "vite": "^6.0"
  }
